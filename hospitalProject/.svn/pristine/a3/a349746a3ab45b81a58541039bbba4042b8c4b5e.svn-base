{% load staticfiles %}
<!--<!DOCTYPE html>-->
<html lang="en">
<head>
    <link rel='stylesheet' href="{% static 'fullcalendar.css' %}">
    <script src="{% static 'jquery.min.js' %}"></script>
    <script src="{% static 'moment.min.js' %}"></script>
    <script src="{% static 'fullcalendar.js' %}"></script>
    <meta charset="UTF-8">

    <script>
    $(document).ready(function() {

    // page is now ready, initialize the calendar...

    $('#calendar').fullCalendar({
        // put your options and callbacks here
        events: '/healthnet/appointmentsJSON/'
    });
    });

    </script>
<style>
.body{
    margin: 0px;
}
.body2{
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
    font-size: 14px;
    line-height: 1.42857143;
    color: #333;
    background-color: #fff;
    padding: 10px;
}
ol.header{
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color:#333333;
}

li.header{
    float: left;
}

li.header a{
    display: block;
    color: white;
    text-align: center;
    padding: 16px;
    text-decoration: none;
    font-size: 14px;
}

li.header a:hover{
    background-color:#111111;
}
.calendar{
    margin: 10px;
}
</style>
</head>
<title></title>
<body class="body">
<div class="body2">
        <ol class="header">
            <li class="header"><a href = {% url 'landing' %}>Home</a></li>
            <li class="header"><a href = {% url 'logout_view' %}>Log Out</a></li>
        </ol>
    </div>
    </br><div class="calendar" id='calendar'></div>
</body>
</html>